{
  "name": "@rika-labs/parallel",
  "version": "0.2.0",
  "description": "A Unix-friendly CLI for the Parallel Search and Extract APIs",
  "keywords": [
    "parallel",
    "search",
    "extract",
    "cli",
    "semantic-search",
    "effect-ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rika-Labs/parallel.git"
  },
  "homepage": "https://github.com/Rika-Labs/parallel#readme",
  "bugs": {
    "url": "https://github.com/Rika-Labs/parallel/issues"
  },
  "author": "Rika Labs",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "parallel": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "lint": "oxlint . --ignore-pattern '.submodules/**'",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@types/bun": "latest",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxlint": "^1.38.0"
  },
  "dependencies": {
    "@effect/cli": "^0.73.0",
    "@effect/platform": "^0.94.1",
    "@effect/platform-bun": "^0.87.0",
    "effect": "^3.19.14"
  },
  "lint-staged": {
    "*.ts": [
      "oxlint --fix"
    ]
  }
}
